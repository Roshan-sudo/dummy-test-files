{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "env": {
        "MONGODB_URL" : "@mongodb_url",
        "API_ROUTE" : "@api_route"
    }
    
}